{
    "name": "icecave/duct",
    "description": "An incremental streaming JSON parser.",
    "keywords": ["json", "parse", "parser", "stream", "event", "evented", "react"],
    "homepage": "https://github.com/IcecaveStudios/duct",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": ">=5.3",
        "eloquent/enumeration": "~3",
        "icecave/collections": "~0.8.0",
        "evenement/evenement": "~1"
    },
    "require-dev": {
        "eloquent/typhoon": "~0.9.0",
        "icecave/archer": "~1"
    },
    "suggest": {
        "eloquent/asplode": "Drop-in exception-based error handling."
    },
    "autoload": {
        "psr-0": {
            "Icecave\\Duct": "src",
            "Icecave\\Duct\\TypeCheck": "src-typhoon"
        }
    },
    "extra": {
        "typhoon": {
            "output-path": "src-typhoon",
            "validator-namespace": "Icecave\\Duct\\TypeCheck",
            "use-native-callable": false
        }
    }
}
