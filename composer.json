{
    "name": "icecave/duct",
    "description": "An incremental streaming JSON parser.",
    "keywords": [
        "json",
        "parse",
        "parser",
        "stream",
        "event",
        "evented",
        "react"
    ],
    "homepage": "https://github.com/IcecaveStudios/duct",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": ">=5.4",
        "eloquent/enumeration": "~5",
        "evenement/evenement": "~2"
    },
    "require-dev": {
        "icecave/archer": "~1",
        "athletic/athletic": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "Icecave\\Duct\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icecave\\Duct\\": "test/benchmarks"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "2.0.x-dev"
        }
    }
}
